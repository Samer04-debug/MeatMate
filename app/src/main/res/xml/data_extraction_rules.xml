<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Include specific directories or files for backup -->
        <include domain="file" path="my_data/important_data.json" />
        <include domain="database" ref="user_data" />

        <!-- Exclude other files from backup -->
        <exclude domain="file" path="cache/" />
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>